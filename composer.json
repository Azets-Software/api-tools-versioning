{
    "name": "zfcampus/zf-versioning",
    "description": "ZF2 Module providing listeners and route prototypes for implementing API versioning",
    "type": "library",
    "license": "BSD-2-Clause",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "rest"
    ],
    "homepage": "https://bitbucket.org/zendframework/zfversioning",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-eventmanager": ">=2.3.0dev",
        "zendframework/zend-http": ">=2.3.0dev",
        "zendframework/zend-modulemanager": ">=2.3.0dev",
        "zendframework/zend-mvc": ">=2.3.0dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "zendframework/zend-config": ">=2.3.0dev",
        "zendframework/zend-servicemanager": ">=2.3.0dev"
    },
    "autoload": {
        "psr-0": {
            "ZF\\Versioning\\": "src/",
            "ZFTest\\": "test/"
        }
    }
}
